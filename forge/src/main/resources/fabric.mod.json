{
  "schemaVersion": 1,
  "id": "mcbot-${mc_version}",
  "version": "${version}",
  "name": "McBot",
  "description": "Adds chat linking between QQ and Minecraft and QQ commands to request server data.",
  "authors": [
    "cnlimiter"
  ],
  "contact": {
    "issues": "https://github.com/cnlimiter/Bot-Connect/issues",
    "sources": "https://github.com/cnlimiter/Bot-Connect"
  },
  "license": "AGPL-3.0",
  "icon": "assets/mcbot/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "cn.evole.mods.mcbot.McBot"
    ]
  },
  "mixins": [
    "mcbot.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.7",
    "fabric": "*",
    "minecraft": "${minecraft_dependency}",
    "java": ">=17"
  }
}
