{
	"schema_version": 1,
	"quilt_loader": {
		"group": "cn.evolvefield.mods",
		"id": "botapi",
		"version": "${version}",
		"metadata": {
			"name": "Bot-Connect",
			"description": "Adds chat linking between QQ and Minecraft and QQ commands to request server data.",
			"contributors": {
				"cnlimiter": "Owner"
			},
			"contact": {
				"homepage": "https://wiki.nova-committee.cn/project-1/doc-1/",
