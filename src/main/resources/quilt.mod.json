		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.16.0-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=1.0.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.18.2"
